{
    "manifest_version": 2,
    "name": "UwUhunter",
    "version": "0.2",
  
    "description": "Why not hunt the UwU?",
  
    "content_scripts": [
      {
        "matches": ["*://*.opensea.io/collection/uwucrew", "https://rt.quantumly.dev/collectable/score/uwucrew/*"],
        "js": ["uwuhunt.js"]
      }
    ],

    "permissions": [
      "webRequest",
      "https://rt.quantumly.dev/collectable/score/uwucrew/*"
    ]
  }